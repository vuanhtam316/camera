<div class="modal fade" id="myModalViewVideo" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content modal-content-one">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title" id="myModalLabel">Introduction to ProgrammingFree</h4>
            </div>
            <div class="modal-body">
                <div class="modal-body" id="click_close">
                    <img class="img-responsive" id="urlImg@(+Model.CAMERA_ID)" data-url="@Model.CAMERA_URL" src="@Model.CAMERA_URL" onload="@("preload('" + Model.CAMERA_URL + "','" + Model.CAMERA_ID + "')")" alt="" title="" />
                </div>
            </div>
        </div>
    </div>
</div>
<script>
    $("#click_close").dblclick(function () {
        $('#myModalViewVideo').modal('hide');
    });
</script>
